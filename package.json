{
  "name": "ackystack-utils",
  "version": "2.0.0",
  "main": "dist/ackystack-utils.js",
  "unpkg": "dist/ackystack-utils.js",
  "module": "dist/ackystack-utils.esm.js",
  "exports": {
    "./jsx-runtime": {
      "main": "./dist/ackystack-utils.esm.js"
    },
    "./jsx-dev-runtime": {
      "main": "./dist/ackystack-utils.esm.js"
    }
  },
  "typings": "dist/types/index.d.ts",
  "files": [
    "src/**/*",
    "dist/*.js",
    "dist/*.map",
    "index.d.ts"
  ],
  "repository": "https://github.com/AckyStack/AckyStack-JS-Utils/tree/next",
  "author": "Xuanzi An",
  "license": "Apache-2.0",
  "scripts": {
    "test": "yarn jest --coverage",
    "build": "yarn rollup -c",
    "build:compress": "yarn rollup -c && yarn gzip-size dist/ackystack-utils.js",
    "build:compat": "yarn rollup --config rollup.config.js",
    "dev": "yarn rollup -c --watch",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/jest": "^26.0.15",
    "codecov": "^3.8.1",
    "cross-env": "^7.0.2",
    "gzip-size-cli": "^4.0.0",
    "jest": "^26.6.3",
    "rollup": "^2.33.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "terser": "^5.5.0",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "yarpm": "^0.2.1"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.0.0",
    "js-base64": "^3.6.0",
    "nanoid": "^3.1.18",
    "qs": "^6.9.4",
    "ts-md5": "^1.2.7"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
